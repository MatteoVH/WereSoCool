{ f: 220, l: 1.0, g: 1.0, p: 0.0}

overtones = {
    O[
    	(5/4, 3.0, 1.0, 1.0),
      	(1/1, 2.0, 1.0, 0.5),
      	(1/1, 0.0, 1.0, -0.5),
  	]
}

thing1 = {
	overtones |
	Sequence [
		AsIs,
		Tm 9/8,
		Tm 5/4,
		Tm 4/3,
		Tm 3/2,
		Tm 5/3,
		Tm 15/8,
		Tm 2,
	] 
	| Length 0.25	
}

thing2 = {
	Sequence [
		Tm 3/2, 
		Tm 5/3,
		Tm 15/8,
		Tm 2
	] 
	| Tm 1/2
	| Gain 2
	> FitLength thing1
}

thing3 = {
	Sequence [
		Tm 9/8, 
		Tm 1/1,
		Tm 15/16,
		Tm 3/4
	] 
	| Tm 1/2
	| Gain 2
	> FitLength thing1
}

main = {
	Overlay [
		thing1,
		thing2,
		thing3
	] 
	| Length 0.5 
	| Tm 3
	| Repeat 3
	| Sequence [AsIs, Tm 4/3, Tm 3/2, Tm 9/8, Tm 3/2] 
	| Sequence [Repeat 2, Reverse, Tm 3]
}
